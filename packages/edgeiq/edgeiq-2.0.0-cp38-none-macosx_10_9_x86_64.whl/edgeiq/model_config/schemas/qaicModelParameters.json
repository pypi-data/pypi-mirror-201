{
    "$id": "https://alwaysai.co/qaicModelParameters.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "qaicModelProperties": {
            "type": "object",
            "required": [
                "architecture",
                "purpose",
                "batch_size"
                ],
            "properties": {
                "architecture": {
                    "type": "string",
                    "$ref": "QaicArchitecture"
                },
                "purpose": {
                    "enum": [
                        "ObjectDetection"
                    ],
                    "type": "string"
                },
                "batch_size": {
                    "type": "number"
                }
            }
        },
        "QaicArchitecture": {
            "$id": "QaicArchitecture",
            "type": "string",
            "enum": [
                "yolov4",
                "yolov3"
            ]
        }
    }
}