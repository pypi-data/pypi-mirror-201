                        "desc": "2nd order contribution to the output variance",
                        "htm": "S<sup><font color=#0000FF>2</font></sup>"
                    },
                    {
                        "name": "S2_conf",
                        "desc": "Confidence interval of 2nd order index",
                        "htm": "CI<sup><font color=#0000FF>2</font></sup>"
                    }
                ],
                "desc": "Sobol Analysis",
                "options": {
                    "calc_second_order": true,
                    "num_resamples": 100,
                    "conf_level": 0.95
                }
            },
            "MORRIS": {
                "outputs": [
                    {
                        "name": "mu",
                        "desc": "The mean elementary effect",
                        "htm": "&#x3BC;"
                    },
                    {
                        "name": "mu_star",
                        "desc": "The absolute of the mean elementary effect",
                        "htm": "&#x3BC;<sup>*</sup>"
                    },
                    {
                        "name": "sigma",
                        "desc": "The standard deviation of the elementary effect",
                        "htm": "&#x3C3;"
                    },
                    {
                        "name": "mu_star_conf",
                        "desc": "The bootstrapped confidence interval",
                        "htm": "CI<sup>&#x3BC;</sup>"
                    }
                ],
                "desc": "One-step-at-a-time method",
                "options": {
                    "num_resamples": 100,
                    "conf_level": 0.95,
                    "num_levels": 4
                }
            },
            "FAST": {
                "outputs": [
                    {
                        "name": "S1",
                        "desc": "1st order sensitivity",
                        "htm": "S<sup><font color=#0000FF>1</font></sup>"
                    },
                    {
                        "name": "ST",
                        "desc": "Total effect",
                        "htm": "S<sup><font color=#0000FF>Total</font></sup>"
                    }
                ],
                "desc": "Fourier Amplitude Sensitivity",
                "options": {
                    "M": 4
                }
            },
            "RBD-FAST": {
                "outputs": [
                    {
                        "name": "S1",
                        "desc": "Confidence interval of 1st order index",
                        "htm": "S<sup><font color=#0000FF>1</font></sup>"
                    }
                ],
                "desc": "Random Balanced Design - Fourier Amplitude Sensitivity Test",
                "options": {
                    "M": 10
                }
            },
            "DGSM": {
                "outputs": [
                    {
                        "name": "vi",
                        "desc": "Sensitivity measure following Sobol and Kucherenko (2009)",
                        "htm": "vi"
                    },
                    {
                        "name": "vi_std",
                        "desc": "vi std",
                        "htm": "vi<sup><font color=#0000FF>std</font></sup>"
                    },
                    {
                        "name": "dgsm",
                        "desc": "Derivative-Based Global Sensitivity Measures",
                        "htm": "DGSM"
                    },
                    {
                        "name": "dgsm_conf",
                        "desc": "Confidence interval of DGSM",
                        "htm": "CI<sup><font color=#0000FF>DGSM</font></sup>"
                    }
                ],
                "desc": " Derivative-based Global Sensitivity Measure",
                "options": {
                    "num_resamples": 1000,
                    "conf_level": 0.95
                }
            },
            "DELTA": {
                "outputs": [
                    {
                        "name": "delta",
                        "desc": "Global importance",
                        "htm": "&#x3B4;"
                    },
                    {
                        "name": "delta_conf",
                        "desc": "Confidence interval of global importance index",
                        "htm": "CI<sup><font color=#0000FF>&#x3B4;</font></sup>"
                    },
                    {
                        "name": "S1",
                        "desc": "1st order sensitivity",
                        "htm": "S<sup><font color=#0000FF>1</font></sup>"
                    },
                    {
                        "name": "S1_conf",
                        "desc": "Confidence interval of 1st order index",
                        "htm": "CI<sup><font color=#0000FF>1</font></sup>"
                    }
                ],
                "desc": "Delta Moment-Independent Analysis",
                "options": {
                    "num_resamples": 10,
                    "conf_level": 0.95
                }
            }
        },
