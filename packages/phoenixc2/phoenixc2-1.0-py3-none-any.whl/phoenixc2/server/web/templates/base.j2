<!doctype html>
<html lang="en">
    <head>
        <title>PhoenixC2 |
            {% block title %}{% endblock %}
        </title>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"
              name="viewport"/>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <link rel="icon" type="image/png" href="/static/images/icon.png">
        <link rel="stylesheet" type="text/css" href="/static/css/styles.css" />
        <!--     Fonts and icons     -->
        <link rel="stylesheet"
              type="text/css"
              href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons"/>
        <link rel="stylesheet"
              href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
        <!-- Material Kit CSS -->
        <link href="/static/css/material-dashboard.css?v=2.1.0" rel="stylesheet" />
        {% block head %}{% endblock %}
    </head>
    <body class="dark-edition">
        <div class="wrapper ">
            <div class="sidebar"
                 data-color="orange"
                 data-background-color="black"
                 data-image="/static/images/icon.png">
                <div class="logo">
                    <a href="/" class="simple-text logo-normal">
                        {% if current_operation %}
                            PhoenixC2 | {{ current_operation.name }}
                        {% else %}
                            PhoenixC2
                        {% endif %}
                    </a>
                </div>
                <div class="sidebar-wrapper">
                    <ul class="nav">
                        <li class="nav-item {% if request.path == "/" %} active{% endif %}">
                            <a class="nav-link" href="/">
                                <i class="material-icons">{{ icons["dashboard"] }}</i>
                                <p>Dashboard</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/operations/" %} active{% endif %}">
                            <a class="nav-link" href="/operations">
                                <i class="material-icons">{{ icons["operations"] }}</i>
                                <p>Operations</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/devices/" %} active{% endif %}">
                            <a class="nav-link" href="/devices">
                                <i class="material-icons">{{ icons["devices"] }}</i>
                                <p>Devices</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/listeners/" %} active{% endif %}">
                            <a class="nav-link" href="/listeners">
                                <i class="material-icons">{{ icons["listeners"] }}</i>
                                <p>Listeners</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/stagers/" %} active{% endif %}">
                            <a class="nav-link" href="/stagers">
                                <i class="material-icons"> {{ icons["stagers"] }}</i>
                                <p>Stagers</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/loaders/" %} active{% endif %}">
                            <a class="nav-link" href="/loaders">
                                <i class="material-icons"> {{ icons["loaders"] }}</i>
                                <p>Loaders</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/modules/" %} active{% endif %}">
                            <a class="nav-link" href="/modules">
                                <i class="material-icons"> {{ icons["modules"] }}</i>
                                <p>Modules</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/users/" %} active{% endif %}">
                            <a class="nav-link" href="/users">
                                <i class="material-icons"> {{ icons["users"] }}</i>
                                <p>Users</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/credentials/" %} active{% endif %}">
                            <a class="nav-link" href="/credentials">
                                <i class="material-icons"> {{ icons["credentials"] }}</i>
                                <p>Credentials</p>
                            </a>
                        </li>
                        <li class="nav-item {% if request.path == "/logs/" %} active{% endif %}">
                            <a class="nav-link" href="/logs">
                                <i class="material-icons"> {{ icons["logs"] }}</i>
                                <p>Logs</p>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="main-panel">
                <!-- Navbar -->
                <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top "
                     id="navigation-example">
                    <div class="container-fluid">
                        <!-- <div class="navbar-wrapper">
             <a class="navbar-brand" href="/">Dashboard</a>
          </div> -->
                        <button class="navbar-toggler"
                                type="button"
                                data-toggle="collapse"
                                aria-controls="navigation-index"
                                aria-expanded="false"
                                aria-label="Toggle navigation"
                                data-target="#navigation-example">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="navbar-toggler-icon icon-bar"></span>
                            <span class="navbar-toggler-icon icon-bar"></span>
                            <span class="navbar-toggler-icon icon-bar"></span>
                        </button>
                        <div class="collapse navbar-collapse justify-content-end">
                            <ul class="navbar-nav">
                                <li class="nav-item dropdown">
                                    <a class="nav-link"
                                       href="javscript:void(0)"
                                       id="navbarDropdownMenuLink"
                                       data-toggle="dropdown"
                                       aria-haspopup="true"
                                       aria-expanded="false">
                                        <i class="material-icons">notifications</i>
                                        <span class="notification">{{ user.unseen_logs | count }}</span>
                                        <p class="d-lg-none d-md-block">Notifications</p>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right"
                                         aria-labelledby="navbarDropdownMenuLink">
                                        {% for message in user.unseen_logs %}
                                            {% if message.user %}
                                                <a class="dropdown-item text-{{ message.status }}"
                                                   href="/logs/{{ message.id }}">{{ message.user }}: {{ message.description }}</a>
                                            {% else %}
                                                <a class="dropdown-item text-{{ message.status }}"
                                                   href="/logs/{{ message.id }}">System: {{ message.description }}</a>
                                            {% endif %}
                                        {% endfor %}
                                        {% if user.unseen_logs | count == 0 %}
                                            <a class="dropdown-item text-success" href="#">No new notifications</a>
                                        {% else %}
                                            <a class="dropdown-item text-warning" onclick="readNotifications()">Mark all as read</a>
                                        {% endif %}
                                    </div>
                                </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link"
                                       href="javascript:;"
                                       id="navbarDropdownProfile"
                                       data-toggle="dropdown"
                                       aria-haspopup="true"
                                       aria-expanded="false">
                                        <img class="profile-picture"
                                             src="/users/{{ user.id }}/picture"
                                             alt="/static/images/icon.png">
                                        <p class="d-lg-none d-md-block">{{user}}</p>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right"
                                         aria-labelledby="navbarDropdownProfile">
                                        <a class="dropdown-item" href="#">Settings</a>
                                        <div class="dropdown-divider"></div>
                                        <a class="dropdown-item" href="/auth/logout">Log out</a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
                <!-- End Navbar -->
                <div class="content">
                    <div class="container-fluid">
                        {% block content %}{% endblock %}
                    </div>
                </div>
            </div>
        </div>
        <!--   Scripts  -->
        <script src="/static/js/default.js"></script>
        <script src="/static/js/core/jquery.min.js"></script>
        <script src="/static/js/core/popper.min.js"></script>
        <script src="/static/js/core/bootstrap-material-design.min.js"></script>
        <script src="/static/js/plugins/perfect-scrollbar.jquery.min.js"></script>
        <!--  Notifications Plugin    -->
        <script src="/static/js/plugins/bootstrap-notify.js"></script>
        <!-- Control Center for Material Dashboard: parallax effects-->
        <script src="/static/js/material-dashboard.js?v=2.1.0"></script>
        {% block scripts %}{% endblock %}
    </body>
    {# flash messages #}
    {% with messages = get_flashed_messages(with_categories=true) %}

    {% if messages %}
    <script>
    {% for category, message in messages %}showNotification("{{ message }}", "{{ category }}");{% endfor %}
    </script>
    {% endif %}
    {% endwith %}

    {% for plugin in plugins %}
    <div>
        <!-- {{ plugin.name }} -->
        {% if not plugins.routes  or request.path[1:] in plugin.routes%}
        {# render unsafe html #}
        {{ plugins[plugin] }}
        {% endif %}
    </div>
    {% endfor %}
</html>
